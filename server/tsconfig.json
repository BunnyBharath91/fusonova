{
    "compilerOptions": {
        "target": "ES2020",
        "module": "nodenext",
        "moduleResolution":"nodenext",
        "outDir": "./dist",
        "rootDir": "./",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@prisma/client/runtime/library": ["./generated/prisma/runtime/client"]
        }
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}